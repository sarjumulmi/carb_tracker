<div class="nested-fields">
  <%= f.label :quantity %>
  <%= f.number_field :quantity %>

  <%= f.label :recipe_id, "Recipe" %>
  <%= f.collection_select :recipe_id, Recipe.all, :id, :name %>

  <%= f.label :category %>
  <%= f.collection_select :category, Entry.categories, :first, :first %>

  <%= link_to_remove_association "Remove Entry", f %>
</div>
